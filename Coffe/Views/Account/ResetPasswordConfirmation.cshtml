@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{
    ViewBag.Title = Localizer["Pass"];
    Layout = "_Layout";
}

<div id="tp-page-header" class="tp-page-header">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>@Localizer["Pass"]</h1>
            </div>
        </div>
    </div>
</div>
<div id="tp-breadcrumb" class="tp-breadcrumb">
    <!-- breadcrumb -->
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <ol class="breadcrumb">
                    <li>
                        <a asp-controller="Home" asp-action="Index">@Localizer["Home"]</a>
                    </li>
                    <li class="active">@Localizer["Pass"]</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-12">
            <h1> <a class="text-primary" asp-action="Login">@Localizer["Login"]</a> </h1>
        </div>
    </div>
</div>