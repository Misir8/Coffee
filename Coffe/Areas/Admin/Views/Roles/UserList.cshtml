@model IEnumerable<Coffe.Models.User>
 


<div class="col-md-12">
    <div class="card">
        <div class="card-header">
            <strong class="card-title">İstifadəçilərin Siyahısı</strong>
        </div>
        <div class="card-body">
            <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Email</td>
                        <td>
                            <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Giriş Hüququ</a>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>